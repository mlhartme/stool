  "manifest": "---\n# Source: stool/templates/deployment.yaml\napiVersion: v1\nkind: ConfigMap\nmetadata:\n  name: stool-log-collector\ndata:\n  fluent.conf: |\n    \u003csource\u003e\n      @type forward\n      port 24224\n      bind 0.0.0.0\n    \u003c/source\u003e\n\n    \u003cmatch stage.format\u003e\n      @type file\n      path /fluentd/log/${stage}/${source_path}\n      \u003cbuffer stage,source_path\u003e\n        @type memory\n        flush_mode interval\n        flush_interval 10s\n      \u003c/buffer\u003e\n      \u003cformat\u003e\n        @type single_value\n      \u003c/format\u003e\n      append true\n      add_path_suffix false\n    \u003c/match\u003e\n---\n# Source: stool/templates/service.yaml\napiVersion: v1\nkind: Service\nmetadata:\n  name: stool\nspec:\n  ports:\n    - name: https\n      nodePort: 31000\n      port: 443\n      targetPort: 8080\n    - name: fluentd-tcp\n      port: 24224\n      protocol: TCP\n      targetPort: 24224\n    - name: fluentd-udp\n      port: 24224\n      protocol: UDP\n      targetPort: 24224\n  selector:\n    app: stool\n  type: NodePort\n---\n# Source: stool/templates/deployment.yaml\napiVersion: apps/v1\nkind: Deployment\nmetadata:\n  name: stool\n  labels:\n    app: stool\nspec:\n  replicas: 1\n  selector:\n    matchLabels:\n      app: stool\n  template:\n    metadata:\n      # https://helm.sh/docs/howto/charts_tips_and_tricks/#automatically-roll-deployments\n      annotations:\n        rollme: \"Smiw5\"\n      labels:\n        app: stool\n    spec:\n      containers:\n      - name: stool\n        image: contargo.server.lan/cisoops-public/stool-server:6.1.0-20201203-143327\n        readinessProbe:\n          httpGet:\n            path: /api/info\n            port: 8080\n          initialDelaySeconds: 5\n          periodSeconds: 5\n        resources:\n          limits:\n            cpu: 2\n          requests:\n            cpu: 200m\n        volumeMounts:\n          - mountPath: /etc/fault\n            name: fault\n          - mountPath: /etc/charts\n            name: charts\n          - mountPath: /var/log/stages\n            name: fluentd-logs\n        env:\n        - name: FQDN\n          value: \"localhost\"\n        - name: REGISTRY_URL\n          value: \"https://mhm:R6ym-jEy3Jk1RxGa4DXA@contargo.server.lan/cisoops-public/stool/localhost/\"\n        - name: KUBERNETES\n          value: \"http://TODO-needed-for-port-forwarding-not-tested\"\n        - name: OPTS\n          value: \"--add-exports=java.naming/com.sun.jndi.ldap=ALL-UNNAMED --illegal-access=deny -agentlib:jdwp=transport=dt_socket,server=y,suspend=n,quiet=n,address=*:5005\"\n        - name: LDAP_URL\n          value: \"\"\n        - name: LDAP_UNIT\n          value: \"\"\n        - name: LDAP_PRINCIPAL\n          value: \"\"\n        - name: LDAP_CREDENTIALS\n          value: \"\"\n        - name: LDAP_SSO\n          value: \"\"\n        - name: ADMIN\n          value: \"\"\n        - name: AUTO_REMOVE\n          value: \"-1\"\n        - name: DEFAULT_EXPIRE\n          value: \"0\"\n        - name: ENGINE_LOG\n          value: \"\"\n        - name: VALUES\n          value: \"networkPolicy:, fluentd:stool\"\n        - name: LOGLEVEL\n          value: \"INFO\"\n        - name: MAIL_HOST\n          value: \"\"\n        - name: MAIL_USERNAME\n          value: \"\"\n        - name: MAIL_PASSWORD\n          value: \"\"\n      - name: collector\n        image: fluent/fluentd:v1.11.2-1.0\n        volumeMounts:\n          - name: fluentd-config\n            mountPath: /fluentd/etc\n          - name: fluentd-logs\n            mountPath: /fluentd/log\n      initContainers:\n      - name: init-charts\n        image: contargo.server.lan/cisoops-public/stool-server:6.1.0-20201203-143327\n        command: [\"sh\"]\n        args: [\"-c\", \"cd /tmp \u0026\u0026 git clone https://bitbucket.1and1.org/scm/cisodevenv/helmcharts.git charts \u0026\u0026 cp -r charts /etc\"]\n        resources:\n          limits:\n            cpu: 1